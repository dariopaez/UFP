<?xml version='1.0' encoding='utf-8'?>
<widget id="unidatos"
    version="1.9.7"
    xmlns="http://www.w3.org/ns/widgets"
    xmlns:cdv="http://cordova.apache.org/ns/1.0">
 
    <name>Unidatos</name>
    <description>
        Software creado por mobilTech
    </description>
    <author email="andres.higuita@mobiltech.com" href="http://www.mobiltech.co">
        
    </author>
    
    <access origin="*" />
    
    <icon src="icon.png" />

    <preference name="Orientation" value="portrait" />
    <preference name="webviewbounce" value="false" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="SplashScreenDelay" value="2000" />
    <preference name="FadeSplashScreenDuration" value="3000" />
    <preference name="android-minSdkVersion" value="16" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="debuggable" value="false" />
    <preference name="android-installLocation" value="auto" />
    <preference name="SplashScreen" value="screen" />
    <preference name="Fullscreen" value="false" />

    
    <plugin name="cordova-plugin-datecs-printer" spec="https://github.com/giorgiofellipe/cordova-plugin-datecs-printer.git"/>
    <plugin name="cordova-plugin-geolocation" spec="https://github.com/apache/cordova-plugin-geolocation.git"/>
    <plugin name="cordova-plugin-camera" spec="https://github.com/apache/cordova-plugin-camera.git"/>
    <plugin name="org.apache.cordova.file-transfer"/>
    <!--<plugin name="cordova.file-transfer" spec="git://git.apache.org/cordova-plugin-file-transfer.git"/>-->

    <plugin name="cordova-plugin-device" spec="~1.1.3" />
    <plugin name="cordova-plugin-console" spec="~1.0.4" />
    <plugin name="cordova-plugin-whitelist" spec="~1.3.0" />
    <plugin name="cordova-plugin-broadcaster" spec="~2.0.6" />

</widget>